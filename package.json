{
  "name": "ng2-breadcrumbs",
  "version": "0.1.281",
  "description": "Breadcrumb generator for Angular 5",
  "main": "./ng2-breadcrumbs.js",
  "scripts": {
    "clear-lib": "rm -rf lib",
    "prepublish": "npm run clear-lib && npm run ngc-build",
    "ngc-build": "ngc -p ./tsconfig-ngc.json",
    "postinstall": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Centroida/ng2-breadcrumb"
  },
  "keywords": [
    "Angular 2",
    "Angular 4",
    "Angular 5",
    "ng",
    "Breadcrumbs"
  ],
  "typings": "./ng2-breadcrumbs.d.ts",
  "author": "Centroida",
  "license": "MIT",
  "homepage": "https://github.com/Centroida/ng2-breadcrumb",
  "devDependencies": {
    "@angular/animations": "~6.1.0",
    "@angular/common": "~6.1.0",
    "@angular/compiler": "~6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "~6.1.0",
    "@angular/forms": "~6.1.0",
    "@angular/http": "~6.1.0",
    "@angular/platform-browser": "~6.1.0",
    "@angular/platform-browser-dynamic": "~6.1.0",
    "@angular/platform-server": "~6.1.0",
    "@angular/router": "6.1.0",
    "@types/core-js": "^2.5.0",
    "@types/node": "~10.5.4",
    "core-js": "~2.5.7",
    "rxjs": "~6.2.2",
    "ts-loader": "~4.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "zone.js": "~0.8.4"
  },
  "dependencies": {}
}
